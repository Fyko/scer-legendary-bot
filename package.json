{
  "name": "ts-template",
  "main": "dist/main.js",
  "module": "src/main.ts",
  "type": "module",
  "scripts": {
    "dev": "bun --watch run src/main.ts ",
    "build": "bun build --target=bun src/main.ts --outfile=dist/main.js",
    "start": "bun run .",
    "lint": "prettier --check . && eslint src --ext ts --format=pretty",
    "format": "prettier --write . && eslint src --ext ts --fix --format=pretty"
  },
  "dependencies": {
    "@elysiajs/server-timing": "^1.1.0",
    "@elysiajs/swagger": "^1.1.5",
    "common-tags": "^1.8.2",
    "csv-parse": "^5.5.6",
    "discord-api-types": "^0.37.94",
    "discord.js": "^14.15.3",
    "drizzle-kit": "^0.27.0",
    "drizzle-orm": "^0.36.0",
    "elysia": "^1.1.23",
    "elysia-ip": "^1.0.7",
    "elysia-rate-limit": "^4.1.0",
    "mysql2": "^3.11.3",
    "pino": "^9.5.0",
    "pino-pretty": "^11.3.0",
    "ulid": "^2.3.0",
    "xksuid": "^0.0.4"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.4",
    "bun-types": "latest",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "eslint": "^8.57.0",
    "eslint-config-neon": "^0.1.62",
    "eslint-formatter-pretty": "^5.0.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.2"
  }
}
