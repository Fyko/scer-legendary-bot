{
  "name": "ts-template",
  "main": "dist/main.js",
  "module": "src/main.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run . --hot",
    "build": "bun build --target=bun src/main.ts --outfile=dist/main.js",
    "start": "bun run .",
    "lint": "prettier --check . && eslint src --ext ts --format=pretty",
    "format": "prettier --write . && eslint src --ext ts --fix --format=pretty"
  },
  "dependencies": {
    "common-tags": "^1.8.2",
    "csv-parse": "^5.5.6",
    "discord.js": "^14.15.3"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.4",
    "bun-types": "latest",
    "eslint": "^8.57.0",
    "eslint-config-neon": "^0.1.62",
    "eslint-formatter-pretty": "^5.0.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.2"
  }
}
